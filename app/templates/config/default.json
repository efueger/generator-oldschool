{
	"manifest": {
		"server": {
			"debug": {
				"request": [ "error" ]
			},
			"connections": {
				"routes": {
					"security": true
				}
			}
		},
		"connections": [{
			"port": 8080,
			"labels": [ "web" ]
		}],
		"registrations": [{
			"plugin": "inert"
		}, {
			"plugin": {
				"register": "./routes/api"
			},
			"options": {
				"routes": {
					"prefix": "/api"
				}
			}
		}, {
			"plugin": {
				"register": "./routes/web"
			}
		}]
	},
	"web": {
		"content": "./src/web"
	}
}
